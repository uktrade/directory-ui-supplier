{
  "host_env_var_prefix": "DIRECTORY_UI_SUPPLIER",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "API_CLIENT_BASE_URL",
      "API_SIGNATURE_SECRET",
      "API_CLIENT_CLASS_NAME",
      "PORT",
      "SECRET_KEY",
      "SECTOR_LINKS_JSON",
      "RECAPTCHA_PUBLIC_KEY",
      "RECAPTCHA_PRIVATE_KEY",
      "ZENDESK_EMAIL",
      "ZENDESK_SUBDOMAIN",
      "ZENDESK_TOKEN",
      "UTM_COOKIE_DOMAIN"
