    "optional": [
      "DEBUG",
      "ZENDESK_TICKET_SUBJECT",
      "GOOGLE_TAG_MANAGER_ID",
      "GOOGLE_TAG_MANAGER_ENV",
      "FEATURE_ADVANCED_MANUFACTURING_ENABLED",
      "FEATURE_MORE_INDUSTRIES_BUTTON_ENABLED",
      "FEATURE_UNSUBSCRIBE_VIEW_ENABLED"
